  "scripts": {
    "install": "napa",
    "init": "npm i inquirer@1.0.3 chalk cli-spinner del babel-cli babel-preset-es2015 && babel-node ./boilerplate/init/index.js",
    "dev": "cross-env NODE_ENV=development gulp --env dev",
    "prod": "cross-env NODE_ENV=production gulp --env prod"
  },
  "directories": {
    "src": "./src",
    "dist": "../dist",
    "test": "./test"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "react"
    ],
    "globals": {
      "ga": true,
      "window": true
    },
    "rules": {
      "comma-dangle": 0
    }
  },
  "config": {
    "verbose": false,
    "port": 3000,
    "browsers": [
      "last 2 version",
      "safari 5",
      "ie 9",
      "opera 12.1",
      "ios 6",
      "android 4"
    ],
    "prodURL": "http://domain.tld",
    "shareImageURL": "http://domain.tld/static/share.png",
    "twitterImage": "https://domain.tld/static/twitter.png",
    "twitterHandle": "@domain",
    "themeColor": "#eeeeee",
    "analyticsUA": "UA-XXXXX-X",
    "developerURL": ""
  },
  "extensions": {
    "scripts": "js",
    "styles": "scss"
  },
  "browserify": {
    "transform": [
      "babelify",
      "aliasify",
      "brfs"
    ]
  },
  "aliasify": {
    "aliases": {
      "root": "./src/scripts",
      "views": "./src/scripts/views",
      "components": "./src/scripts/components",
      "spa": "./src/scripts/spa"
    }
  }
}
